@model WebService.Models.EmployeeDet

@{
    ViewBag.Title = "View1";
}

<h2>View1</h2>
<script src="http://ajax.aspnetcdn.com/ajax/jQuery/jquery-2.0.3.min.js"></script>
<script>
    $document.ready(function()
    {
    $.ajax({
        url:"",
        type:"GET",
        datatype:"json",
        success: function(result)
        {
            $.each(result, function (key, item) {  
                htmlContent =  "<tr><td>" + item.Id + "</td><td>" + item.Name + "</td><td>" + item.Experiance + "</td></tr>"; 
            })}
            })
            })




</script>